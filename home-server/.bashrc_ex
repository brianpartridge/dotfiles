# PATH
export PATH=~/bin:$PATH

# Important Folders
export drobo=/Volumes/DroboFS/tdb113070198/1/Public
export media=$drobo/media
export media=/Volumes/Public/media
export dow=~/Transfers/complete
export dowi=~/Transfers/incomplete
export mov=$media/movies-misc
export movc=$media/movies-classics
export tv=$media/tv-freeform
export tvo=$media/tv-organized

# Alias to quickly navigate folders
alias dow='cd $dow'
alias dowi='cd $dowi'
alias mov='cd $mov'
alias movc='cd $movc'
alias tv='cd $tv'
alias tvo='cd $tvo'
